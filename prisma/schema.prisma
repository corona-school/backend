  status           String    @default("pending") @db.VarChar
  token            String    @unique @db.VarChar
  reminderSentDate DateTime? @db.Timestamp(6)
  pupilId          Int?      @unique
  pupil            pupil?    @relation(fields: [pupilId], references: [id])
